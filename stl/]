#include<iostream>
#include<vector>
#include<algorithm>
#include<iterator>

using namespace std;

void print(vector<int> ivec)
{
    copy(ivec.begin(), ivec.end(), ostream_iterator<int>(std::cout, " "));
    std::cout << "\n";
}

void test1()
{
    int arr[5] = {1,2,3,4,5};
    vector<int> ivec;
    ivec.assign(arr, arr+5);
    print(ivec);
    vector<int>::reverse_iterator reit = ivec.rbegin();
    for(reit = ivec.rbegin(); reit != ivec.rend(); reit++)
        std::cout << *reit << std::endl;

}

int main()
{
    test1();
    std::cout << "Hello world!" << std::endl;
    return 0;
}
